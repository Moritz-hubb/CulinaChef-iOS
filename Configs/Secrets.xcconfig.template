// ----- Supabase Configuration -----
// Get from: Supabase Dashboard → Settings → API
SUPABASE_URL = https:/$()/your-project.supabase.co
SUPABASE_ANON_KEY = YOUR_SUPABASE_ANON_KEY_HERE

// ----- Sentry Configuration -----
// Get from: https://sentry.io/settings/projects/YOUR_PROJECT/keys/
// Leave empty to disable Sentry
SENTRY_DSN = YOUR_SENTRY_DSN_HERE

// ----- OpenAI Configuration -----
// ⚠️ DO NOT USE! OpenAI key belongs ONLY in the backend!
// The iOS app proxies all AI requests through the backend
OPENAI_API_KEY = unused-backend-only

